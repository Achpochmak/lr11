<p id="notice"><%= notice %></p>

<h1>Polyndroms</h1>

<table>
  <thead>
    <tr>
      <th>Index</th>
      <th>Num</th>
      <th>Rev</th>
      <th>Theory</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @polyndroms.each do |polyndrom| %>
      <tr>
        <td><%= polyndrom.index %></td>
        <td><%= polyndrom.num %></td>
        <td><%= polyndrom.rev %></td>
        <td><%= polyndrom.theory %></td>
        <td><%= link_to 'Show', polyndrom %></td>
        <td><%= link_to 'Edit', edit_polyndrom_path(polyndrom) %></td>
        <td><%= link_to 'Destroy', polyndrom, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Polyndrom', new_polyndrom_path %>
